import java.io.*;
import java.util.*;
public class RevString()
{
   public static void main(String [] args)
   {
     String strorg = "Hello GuviTeam";
     system.out.println("Original String" + strorg);
      strorg = new StringBuffer(strorg).reverse().toString();
      system.out.println("Reverse String is:" + strorg);
   }
}   
      
